<?php

/**
 * @file
 * Scald Youtube Installation
 */

/**
 * Implements hook_install().
 */
function atom_package_gallery_install() {
  scald_add_type('atom_package', 'Atom package', 'Atom for package');
}

/**
 * Implements hook_uninstall().
 */
function atom_package_gallery_uninstall() {
  drupal_load('module', 'scald');
  scald_remove_type('atom_package');
}
